{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx", // React가 아니라면 이건 제거
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "outDir": "./dist", // 혹시 이게 "./src"인가요?
    "types": ["vite-plugin-svgr/client"],

  },
  "include": ["src","svg.d.ts"],
  "exclude": ["node_modules"]
}
